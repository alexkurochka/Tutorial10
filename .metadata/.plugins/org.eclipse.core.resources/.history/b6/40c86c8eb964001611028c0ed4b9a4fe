
public class App {
	public static void main(String[] args) {
		String[] texts = new String[3];
		
		texts[0] = "Hello";
		texts[1] = "to";
		texts[2] = "you.";
		System.out.println(texts[2]);
		
		String[] fruits = {"apple","kiwi","cherry"};
		for(String fruit: fruits){
			System.out.println(fruit);
			
		String text = null;
		}
	}
}
