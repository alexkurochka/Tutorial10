
public class Application {

	public static void main(String[] args) {
		int myInteger = 90;
		short myShort = 990;
		char myChar = 't';
		
		double myDouble = 22365;
		
		
		System.out.println(myInteger);
		System.out.println(myShort);
		System.out.println(myChar);
		System.out.println(myDouble);
	}

}
